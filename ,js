     
    for (let i = 1; i <= 100; i++) {
        let str = "";
        if(i%3===0 && i===5) str +="fizzbuzz"
        else if (i % 3 === 0) str += "fizz"
        else if (i % 5 === 0) str += "buzz"
        else str+=i
        console.log(str);        
        } 
    
    
        sum=0
        for (var x = 0; x < 1000; x++){
         if (x%3===0 && x%5===0){
            sum+=x
            
      } console.log(x)
    }